    "hapi-pino": "^4.0.4",
    "inert": "^5.1.0",
    "mongoose": "^5.2.6",
    "pino": "^4.17.6",
    "request": "^2.87.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-pug": "file:parcel-plugin-pug",
    "pug": "^2.0.3",
    "page": "^1.8.6",
    "whatwg-fetch": "^2.0.4"
  },
  "repository": {
