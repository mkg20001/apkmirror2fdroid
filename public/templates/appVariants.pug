h2 Variants/Channels
ul
  each variant in variants
    li
      span
        input(id=variant.id, type="checkbox", checked=variant.enabled || (!id && variant.isLatest))
      if variant.isLatest
        span <b>#{variant.androidVer} #{variant.arch} #{variant.dpi} - #{variant.version}</b>
      unless variant.isLatest
        span #{variant.androidVer} #{variant.arch} #{variant.dpi} - #{variant.version}
