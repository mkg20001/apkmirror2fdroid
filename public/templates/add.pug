div(class="jumbotron")
  h1= app.name
  h4= dev.display

  ul
    li
      a(href=app.url) APKDroid
    li
      a(href=play.url) PlayStore
    li
      a(href=dev.url) Developer Page

  h2 Description
  each note in notes
    tt=note
    br

  h2 Variants
  ul
    each variant in variants
      li
        a(href=variant.addUrl)= variant.name
